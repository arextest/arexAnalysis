{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "id",
    "dashboardId",
    "isAiAlert",
    "dashboardUid",
    "dashboardSlug",
    "panelId",
    "name",
    "state",
    "newStateDate",
    "evalDate",
    "evalData",
    "executionError",
    "settings",
    "url"
  ],
  "properties": {
    "dashboardId": {
      "type": "integer"
    },
    "dashboardSlug": {
      "type": "string"
    },
    "dashboardUid": {
      "type": "string"
    },
    "evalData": {
      "type": "null"
    },
    "evalDate": {
      "format": "date-time",
      "type": "string"
    },
    "executionError": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "isAiAlert": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "newStateDate": {
      "format": "date-time",
      "type": "string"
    },
    "panelId": {
      "type": "integer"
    },
    "settings": {
      "type": "object",
      "required": [
        "appid",
        "conditions",
        "datasourceName",
        "emailGroups",
        "executionErrorState",
        "for",
        "frequency",
        "handler",
        "level",
        "name",
        "noDataState",
        "notifications",
        "notificationsV2",
        "users"
      ],
      "properties": {
        "appid": {
          "type": "string"
        },
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["evaluator", "operator", "query", "reducer", "type"],
            "properties": {
              "evaluator": {
                "type": "object",
                "required": ["alertLevel", "params", "timeRange", "type"],
                "properties": {
                  "alertLevel": {
                    "type": "string"
                  },
                  "params": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    }
                  },
                  "timeRange": {
                    "type": "array"
                  },
                  "type": {
                    "type": "string"
                  }
                }
              },
              "operator": {
                "type": "object",
                "required": ["type"],
                "properties": {
                  "type": {
                    "type": "string"
                  }
                }
              },
              "query": {
                "type": "object",
                "required": [
                  "datasourceId",
                  "datasourceName",
                  "datasourceType",
                  "model",
                  "params"
                ],
                "properties": {
                  "datasourceId": {
                    "type": "number"
                  },
                  "datasourceName": {
                    "type": "string"
                  },
                  "datasourceType": {
                    "type": "string"
                  },
                  "model": {
                    "type": "object",
                    "required": [
                      "alertQuery",
                      "database",
                      "dateLoading",
                      "dateTimeColDataType",
                      "dateTimeType",
                      "datetimeLoading",
                      "disableSliceReq",
                      "fillNullPoint",
                      "format",
                      "formattedQuery",
                      "interval",
                      "intervalFactor",
                      "query",
                      "rawQuery",
                      "refId",
                      "round",
                      "table",
                      "tableLoading"
                    ],
                    "properties": {
                      "alertQuery": {
                        "type": "string"
                      },
                      "database": {
                        "type": "string"
                      },
                      "dateLoading": {
                        "type": "boolean"
                      },
                      "dateTimeColDataType": {
                        "type": "string"
                      },
                      "dateTimeType": {
                        "type": "string"
                      },
                      "datetimeLoading": {
                        "type": "boolean"
                      },
                      "disableSliceReq": {
                        "type": "boolean"
                      },
                      "fillNullPoint": {
                        "type": "boolean"
                      },
                      "format": {
                        "type": "string"
                      },
                      "formattedQuery": {
                        "type": "string"
                      },
                      "interval": {
                        "type": "string"
                      },
                      "intervalFactor": {
                        "type": "number"
                      },
                      "query": {
                        "type": "string"
                      },
                      "rawQuery": {
                        "type": "string"
                      },
                      "refId": {
                        "type": "string"
                      },
                      "round": {
                        "type": "string"
                      },
                      "table": {
                        "type": "string"
                      },
                      "tableLoading": {
                        "type": "boolean"
                      }
                    }
                  },
                  "params": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              },
              "reducer": {
                "type": "object",
                "required": ["params", "type"],
                "properties": {
                  "params": {
                    "type": "array"
                  },
                  "type": {
                    "type": "string"
                  }
                }
              },
              "type": {
                "type": "string"
              }
            }
          }
        },
        "datasourceName": {
          "type": "string"
        },
        "emailGroups": {
          "type": "array",
          "items": {}
        },
        "executionErrorState": {
          "type": "string"
        },
        "for": {
          "type": "string"
        },
        "frequency": {
          "type": "string"
        },
        "handler": {
          "type": "integer"
        },
        "level": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "noDataState": {
          "type": "string"
        },
        "notifications": {
          "type": "array",
          "items": {}
        },
        "notificationsV2": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "emailGroups",
              "isDefault",
              "message",
              "notifications",
              "oncall",
              "sendOwnerAdmin",
              "splitNotice",
              "users"
            ],
            "properties": {
              "emailGroups": {
                "type": "array"
              },
              "isDefault": {
                "type": "boolean"
              },
              "message": {
                "type": "string"
              },
              "notifications": {
                "type": "array"
              },
              "oncall": {
                "type": "object"
              },
              "sendOwnerAdmin": {
                "type": "boolean"
              },
              "splitNotice": {
                "type": "boolean"
              },
              "users": {
                "type": "array"
              }
            }
          }
        },
        "users": {
          "type": "array",
          "items": {}
        }
      }
    },
    "state": {
      "type": "string"
    },
    "url": {
      "type": "string"
    }
  }
}
